dbLoadRecords("ecmcAsynGenericAnalogOutput.db","P=$(ECMC_PREFIX),PORT=$(ECMC_ASYN_PORT),ADDR=0,TIMEOUT=1,MASTER_ID=$(ECMC_EC_MASTER_ID),SLAVE_POS=$(ECMC_EC_SLAVE_NUM),HWTYPE=$(ECMC_EC_HWTYPE),NAME=ec$(ECMC_EC_MASTER_ID).s${ECMC_EC_SLAVE_NUM}.OUT_FLOAT64_2,REC_SUFFIX=OUT_FLOAT64_2,TYPE=asynFloat64")
