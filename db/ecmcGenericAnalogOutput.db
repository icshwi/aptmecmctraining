record(ao,"$(P)EC$(SLAVE_POS)-$(TYPE)-$(REC_SUFFIX)"){
	field(DTYP,"stream")
	field(OUT,"@ecmcEthercat.proto setPdoEntry($(SLAVE_POS),$(NAME)) $(PORT)")
	field(FLNK,"$(P)GetEC$(SLAVE_POS)$(TYPE)_$(NAME).PROC")
}

record(ai,"$(P)EC$(SLAVE_POS)-$(TYPE)-$(REC_SUFFIX)-RB"){
	field(DTYP,"stream")
	field(INP,"@ecmcEthercat.proto getPdoEntry($(SLAVE_POS),$(NAME)) $(PORT)")
	field(SCAN,"1 second")
}
