record(bi,"$(REC_NAME)")
{
   field(PINI, "$(PINI=1)")
   field(DESC, "$(DESC=$(HWTYPE): Binary Input)")
   field(DTYP, "$(DTYP=asynInt32)")
   field(SCAN, "$(SCAN=I/O Intr)")
   field(INP,  "$(INP)")
   field(ZNAM, "$(ZNAM=Zero)")
   field(ONAM, "$(ONAM=One)")
   field(ZSV,  "$(ZSV=NO_ALARM)")
   field(OSV,  "$(OSV=NO_ALARM)")
   field(FLNK, "$(FLNK=)")
}


