
record(bo,"$(P)MCU-ErrRst"){
	field(DTYP,"stream")
	field(OUT,"@ecmcControllerGeneral.proto setGeneralErrorReset() $(PORT)")
	field(ZNAM,"FALSE")
	field(ONAM,"TRUE")
	field(DOL,"0")
}

record(stringin,"$(P)MCU-ErrMsg"){
	field(DTYP,"stream")
	field(INP,"@ecmcControllerGeneral.proto getGeneralErrorMessage() $(PORT)")
        field(SCAN, "1 second")
}

record(ai,"$(P)MCU-ErrId"){
	field(DTYP,"stream")
	field(INP,"@ecmcControllerGeneral.proto getGeneralErrorId() $(PORT)")
	field(SCAN,"1 second")
}

